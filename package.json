{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "shell": "docker-compose exec app sh",
    "console": "docker-compose exec app bin/rails console",
    "release": "yarn publish"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "texterify-api",
  "author": "Christoph Werner @chrztoph",
  "description": "Node.js library for the Texterify API.",
  "module": "dist/texterify-api.esm.js",
  "size-limit": [
    {
      "path": "dist/texterify-api.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/texterify-api.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/node-fetch": "^2.5.8",
    "husky": "^5.0.9",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "query-string": "^6.14.0"
  }
}
